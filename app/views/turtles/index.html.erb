<br><br><br><br><br>
<div class="container">
  <div class="cards">
    <% @turtles.each do |turtle| %>
    <div class="turtle-card">
      <%= link_to turtle.first_name, turtle_path(turtle), class: "turtle-link-names"%>
      <%= link_to turtle.last_name, turtle_path(turtle), class:"turtle-link-names" %>
      <%= @little_description.sample %>
      <%= link_to (image_tag 'cuteturtle.jpg', height: 150), turtle_path(turtle)%>
    </div>
    <% end %>
  </div>
</div>
